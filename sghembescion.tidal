
-- SGHEMBESCION
hush

setcps (130/60/4)

d2 $ slow 2 $ n (scale "major" ("[-1,3,8]" + "1 4 5 2") + "<e3 f3 fs3>") # s "superpiano"

d3 $ slow 2 $ n (scale "major" ("<[[4 [2 3] 2 [7 8] 5] 6 [7 8 [9 8] 7]] \
                                \ [[[8 9 [10 11]] [12 [11 11] 12]] [10 8*2 5*3]]>") + "[<e5 f5 fs5>]") # s "superpiano"

d3 $ slow 2 $ n (scale "major" ("[4 5*2 3*3 -1*4 1*5 3*4 2*5 1*2]") + "<e5 f5 fs5>") # s "superpiano"
