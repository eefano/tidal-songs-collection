
hush

setcps (110/60/4)

let tone = "b4"

d2 $ slow 4 $ n (scale "major" ("1 -2 0 -3") + tone + "-12") # s "supersaw" # amp 0.8

d3 $ fast 0.5 $ n (scale "major" ("[-6,1,4,8]" + "1 4 5 2 3 2 5 3 2 1 1 4") + tone) # s "superpiano" # amp 0.6

d1 $ slow 8 $ n (scale "major" ("1 [5 6] 2 [6 6 4] 7 [-2 -4] 0 -3") + tone + "12" ) # s "supersquare" # amp 0.4

